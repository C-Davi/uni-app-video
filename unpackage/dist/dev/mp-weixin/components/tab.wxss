
.tab{
	height: 50px;
	width: 100%;
	position: fixed;
	bottom: 0;
	left: 0;
	z-index: 21;
}
.tab-box{
	float: left;
	width: 20%;
	color: #FFFFFF;
	text-align: center;
	height: 50px;
	line-height: 50px;
	font-size: 20px;
}
.icon-box{
	width: 60%;
	height: 24px;
	background: #FFFFFF;
	color: #000000;
	margin: 14px 20%;
	line-height: 24px;
	border-radius: 5px;
	font-size: 15px;
}

